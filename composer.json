{
	"name": "psesd/cascade",
	"description": "CRM",
	"keywords": ["yii", "crm"],
	"homepage": "http://psesd.org",
	"type": "project",
	"license": "BSD-3-Clause",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"psesd/cascade-lib": "*",
		"psesd/cascade-core-types": "*",
		"psesd/psesd-cascade-modules": "*"
	},
	"extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
	},
	"repositories": [
	   {
		    "type": "vcs",
		    "url": "https://github.com/PSESD/psesd-cascade-modules.git"
	   }
	],
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
		"yiisoft/yii2-jui": "*",
		"mustangostang/spyc": "*",
		"fabpot/php-cs-fixer": "@stable",
		"canis/yii2-doc-helper": "*"
	}
}
